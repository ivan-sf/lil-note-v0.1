<%- include ('partials/header') %>
<div class="col-md-4 offset-md-4">
    <h1 class="text-center">Create new Lil Note</h1>

    <form role="form" method="post">
        <div class="form-group">
            <label for="title">Title</label>
            <input type="text" name="title" id="title" class="form-control" placeholder="Entry new Note" required>  
        </div>
        <div class="form-group">
            <label for="body">Entry Text</label>
            <textarea name="body" id="body" class="form-control" rows="3" required></textarea>
        </div>
        <div class="form-group">
            <input type="submit" value="POST" class="btn btn-success">
        </div>
    </form>

</div>
<%- include ('partials/footer') %>

